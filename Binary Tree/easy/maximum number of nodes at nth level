//approach1
int numberOfNodes(int N){
    // Write your code here.
    int nodes=1<<(N-1);
    return nodes;
}


//approach2
int pow(int N)
{
    int counter=1;
    for(int i=0;i<N-1;i++)
    {
        counter*=2;
    }
    return counter;
}

int numberOfNodes(int N){
    // Write your code here.
    int res=pow(N);
    return res;
}
